.main {
	height:100%;
	font-size:32rpx;
}

.head image {
	width:256rpx;
	height:256rpx;
	margin-top:130rpx;
}
.head view {
	margin:24rpx 0 120rpx;
}

.page-main {
	width:92%;
	margin:0 auto;
}
.item {
	position: relative;
	margin-top: 0;
	margin-bottom: 36rpx;
}
.input {
	display: block;
	box-sizing: border-box;
	width:690rpx;
	height:96rpx;
	line-height: 96rpx;
	text-align: left;
	padding-left: 80rpx;
	border:thin solid #888;
	border-radius: 20rpx;
}
.place {
	color:#888;
}
.sub {
	background: #169BD5;
	color:#fff;
}
.l {
	text-align: left;
	margin-top: 16rpx;
}
.svg {
	position: absolute;
	top:20rpx;
	left:20rpx;
	width:50rpx;
	height:50rpx;
}
.r,.l {
	display: inline-block;
	width:48%;
}
.r {
	text-align: right;
}

.bg-img {
	position: fixed;
	top:0;
	left:0;
	z-index:-10;
	width:100%;
	min-height:1200rpx;
}
.a {
	color:blue;
}