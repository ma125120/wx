    "bundle-loader": "^0.5.0",
    "css-loader": "^0.15.6",
    "eslint": "^1.1.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "highlight.js": "^9.0.0",
    "node-sass": "^3.4.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.0.0-rc5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.0"
